@if (Model.HasSeasonBests)
{
    <h3 class="text-center">
        Season Bests
    </h3>
    foreach (var season in Model.Seasons)
    {
        var outdoorRunning = Model.OutdoorRunningSeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var outdoorRunningRelay = Model.OutdoorRunningRelaySeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var outdoorField = Model.OutdoorFieldSeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var outdoorFieldRelay = Model.OutdoorFieldRelaySeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var indoorRunning = Model.IndoorRunningSeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var indoorRunningRelay = Model.IndoorRunningRelaySeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var indoorField = Model.IndoorFieldSeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var indoorFieldRelay = Model.IndoorFieldRelaySeasonBests.FirstOrDefault(x => x.Season.Equals(season))?.Performances;
        var hasOutdoor = (outdoorRunning != null && outdoorRunning.Any()) || (outdoorRunningRelay != null && outdoorRunningRelay.Any()) || (outdoorField != null && outdoorField.Any()) || (outdoorFieldRelay != null && outdoorFieldRelay.Any());
        var hasIndoor = (indoorRunning != null && indoorRunning.Any()) || (indoorRunningRelay != null && indoorRunningRelay.Any()) || (indoorField != null && indoorField.Any()) || (indoorFieldRelay != null && indoorFieldRelay.Any());

        <h4 class="text-center">
            @season.Name Season
        </h4>

        @if (hasOutdoor)
        {
            <table class="table table-hover table-striped">
                <caption>@Model.Athlete.Name's @season.Name Season Outdoor Bests</caption>
                <thead>
                <tr>
                    <th colspan="4" class="text-center">
                        @season.Name Season Outdoor Bests
                    </th>
                </tr>
                <tr>
                    <th scope="col" class="text-center">Event</th>
                    <th scope="col" class="text-center">Performance</th>
                    <th scope="col" class="text-center">Date</th>
                    <th scope="col" class="text-center">Meet</th>
                </tr>
                </thead>
                <tbody>
                @if (outdoorRunning != null)
                {
                    foreach (var performance in outdoorRunning)
                    {
                        <tr>
                            <th>@performance.Event.Name</th>
                            <td>
                                @performance.Performance.Time
                                @if (performance.Performance.SchoolRecord)
                                {
                                    <span class="badge bg-warning-subtle" data-bs-toggle="tooltip" data-bs-placement="right" title="School Record">üèÜ</span>
                                }
                                @if (performance.Performance.PersonalBest)
                                {
                                    <span class="badge bg-warning-subtle" data-bs-toggle="tooltip" data-bs-placement="right" title="Personal Record">‚≠ê</span>
                                }
                            </td>
                            <td>@performance.Performance.Meet.Date.ToString("MM/dd/yyyy")</td>
                            <td>
                                <a title="@performance.Performance.Meet.Name" asp-controller="Meets" asp-action="Details" asp-route-meetName="@performance.Performance.Meet.UrlName">
                                    @performance.Performance.Meet.Name @@ @performance.Performance.Meet.Location
                                </a>
                            </td>
                        </tr>
                    }
                }
                @if (outdoorRunningRelay != null)
                {
                    foreach (var performance in outdoorRunningRelay)
                    {
                        <tr>
                            <th>@performance.Event.Name</th>
                            <td>
                                @performance.Performance.Time
